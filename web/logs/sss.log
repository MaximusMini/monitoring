INSERT INTO vk_posts(id_group_post, id_post, id_group, link_post, is_pinned, count, date_unix, date_post, time_post, text_post, comments, likes, reposts, views) VALUES (                            '97180786_184759',                            184759,                            97180786,                            'https://vk.com/public97180786?w=wall-97180786_184759',                            0,                            26314,                            1545120010,                            '18.12.2018',                            '11:00',                            'Беларусы пра ўльтыматум Расіі: Незалежнасць не прадаецца за танную нафту«Наша Ніва» запыталася ў мінакоў, як яны ставяцца да частковай страты беларускага суверэнітэту ў абмен на танную нафту.  Між Беларуссю і Расіяй абвастрыліся адносіны праз цэны на энерганосьбіты: спачатку ў Санкт-Пецярбургу Аляксандр Лукашэнка схапіўся наўпрост з Уладзімірам Пуціным па газавым пытанні, потым віцэ-прэм\'ер Расіі Дзмітрый Козак адмовіўся нават абмяркоўваць кампенсацыю страт ад падатковага манеўру «без далейшай інтэграцыі».  Падсумаваннем пазіцый Расіі стаў ультыматум Мядзведзева ў Брэсце, дзе той назваў «два магчымыя сцэнары для Беларусі», сярод якіх — «інтэграцыя» і адмова Беларусі ад уласнай валюты, судоў і іншых уласных дзяржаўных інстытутаў на карысць «супольных».  Другі варыянт прадугледжвае «цяперашні стан інтэграцыі», што, можна меркаваць, ставіць кропку на жаданні Беларусі і надалей атрымліваць фінансавую падтрымку Расіі ў выглядзе танных энэрганосбітаў.  Што пра гэта думаюць беларусы? Ці гатовыя яны плаціць больш за камунальныя паслугі, абы толькі не аддаваць суверэнітэт?  Глядзіце ў апытанцы.',                            0,                            2,                            0,                            478                            )
