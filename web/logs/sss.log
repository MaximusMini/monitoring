INSERT INTO vk_posts(id_group_post, id_post, id_group, link_post, is_pinned, count, date_unix, date_post, time_post, text_post, comments, likes, reposts, views) VALUES (                            '99725619_132861',                            132861,                            99725619,                            'https://vk.com/club99725619?w=wall-99725619_132861',                            0,                            44680,                            1545118466,                            '18.12.2018',                            '10:34',                            'В Николаеве украинские чиновники не могут снести последний памятник Ленину  В украинском городе Николаев обнаружили уцелевший памятник Владимиру Ленину, и теперь власти не знают, что с ним делать. Об этом сообщает местный сайт 0512.  Монумент находится на территории бывшего пансионата — комплекса дома "Коммунар", на частной земле. Между тем, владелец участка снести памятник отказывается.  У нас такой вопрос каверзный, на Спортивной, 13 обнаружили памятник Ленина и все пишут предписания по его демонтажу. Мы зашли в тупик, собственник говорит: Это моя частная территория и я хочу, чтобы он здесь стоял  — заявил глава Заводской райадминистрации Виктор Дмитрук в ходе аппаратного совещания.  При этом украинский чиновник отметил, что пока нет ясности, что делать с памятником, чтобы выполнить действующий в стране закон о декоммунизации.  И он стоит, мы вокруг этого ходим, но сделать ничего не можем  — сказал Дмитрук.  В свою очередь первый заместитель городского головы Владимир Крыленко поручил чиновникам разобраться и найти решение, как законным путем можно демонтировать памятник.',                            2,                            12,                            1,                            378                            )
