INSERT INTO vk_posts(id_group_post, id_post, id_group, link_post, is_pinned, count, date_unix, date_post, time_post, text_post, comments, likes, reposts, views) VALUES (                            '22639447_540938',                            540938,                            22639447,                            'https://vk.com/club22639447?w=wall-22639447_540938',                            0,                            168472,                            1545143832,                            '18.12.2018',                            '17:37',                            'ℹ [club22639447|ПАЦЫЕНТ ЗАГІНУЎ У ГОМЕЛЬСКАЙ ЛЯКАРНІ ПРЫ ДЗІЎНЫХ АБСТАВІНАХ] Цела мужчыны знайшлі ў дзень ягонай меркаванай выпіскі.                      Гомельскі гарадскі аддзел Следчага камітэта праводзіць праверку па факце смерці мужчыны ў адной з медыцынскіх устаноў Гомеля, паведаміла БелТА афіцыйная прадстаўніца УСК Гомельскай вобласці Марыя Крываногава.Жыхар Жлобінскага раёна 1957 года нараджэння праходзіў лячэнне ў лякарні абласнога цэнтра. 14 снежня, у дзень ягонай меркаванай выпіскі, цела мужчыны было знойдзена ў будынку медустановы. Абставінаў, якія паказваюць на крымінальны характар смерці, цяпер не ўстаноўлена, адзначыла Марыя Крываногава.Следчыя высвятляюць абставіны здарэння, апытваюць сваякоў, медыцынскіх работнікаў і пацыентаў медустановы.#Пациент #погиб в #гомельской #больнице при странных обстоятельствах   j.mp/2R50XJX',                            0,                            2,                            0,                            529                            )
