INSERT INTO vk_posts(id_group_post, id_post, id_group, link_post, is_pinned, count, date_unix, date_post, time_post, text_post, comments, likes, reposts, views) VALUES (                            '97180786_184860',                            184860,                            97180786,                            'https://vk.com/public97180786?w=wall-97180786_184860',                            0,                            26314,                            1545150603,                            '18.12.2018',                            '19:30',                            'Прызнаўся ў 87 згвалтаваннях жанчын… А пачынаў з хатняга гвалту / Кожны з насЯк вядома, беларуская міліцыя спыніла працу над праектам Закону «Аб супрацьдзеянні хатняму гвалту». Хоць аналагічныя заканадаўчыя акты прынятыя ў больш чым ста краін свету.  Маньяк Алег Беднякоў, які прызнаўся ў 87 згвалтаваннях і атрымаў 18 год пазбаўлення волі, пачынаў менавіта з хатняга гвалту.  «Мне вядома пра тое, што ён гвалціў жанчын… Гэта была я. Мне было гадоў 8-9. Я так разумею, ён не любіў, калі яму адмаўляюць», – прызнаецца блізкая свачка асуджанага.  Жанчыны цягам перадачы расказвалі пра свае раны, звязаныя з хатнім гвалтам. Цягам праграмы ў тым жа прызнаецца і экспертка Лілія Ахрэмчык:  «Мама біла мяне, зневажала. Адбывалася тое ж, пра што іншыя расказваюць у студыі».  Сярод ахвяр хатняга гвалту ў перадачы выступіў жыхар Менскага раёну Дзмітрый Шымановіч. Ён кажа, што разам з дзецьмі цярпіць псіхалагічны і фізічны гвалт з боку былой жонкі. Дарэчы, амаль паўнагадовая дачка і сын жывуць разам з ім. Але суд у чарговы раз пастанавіў перадаць 10-гадовага Ігната маме. Дзіця, паводле словаў бацькі, супраць. Праз гэта Ігнат не можа вучыцца ў школе. Яго і бацьку проста не пускаюць у памяшканне школы. Хлопчык раней ужо прапусціў цэлы навучальны год.  «Калі ён ішоў у першы клас, яго мама напісала заяву, каб яго не бралі. Таму замест 4 класаў Ігнат скончыў толькі тры. І сёлета ён прапусціў ужо цэлую чвэрць!» – абураецца Дзмітрый Шымановіч.  Гутарка пра школу №142, якая знаходзіцца ў сталічным мікрараёне Шабаны.  Што цікава, яшчэ адна экспертка, Ілона Навіцкая, прызнаецца, што трапіла ў аналагічную сітуацыю. Яе былы муж, бізнэсовец з добрымі сувязямі ва ўладзе, папросту забраў у яе сумесную дачку. А пасля суд стаў на бок таты. Сама спадарыня Ілона ўжо некалькі год практычна не можа бачыцца з дачкой.  У новым выпуску ток-шоу «Кожны з нас» сваймі гісторыямі адкрыта дзеляцца беларусы, якія перажылі хатні гвалт.',                            1,                            1,                            1,                            385                            )
